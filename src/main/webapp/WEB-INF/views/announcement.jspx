<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/subject" var="subj"/>    <div class="container">        <h3>${announcement.title}</h3>        <sf:form method="post" action="${announcement.id}/delete">            <div class="form-group btn-toolbar">                <a class="btn btn-xs btn-success" role="button" href="${announcement.id}/update">Update</a>                <button class="btn btn-xs btn-danger">Delete</button>            </div>        </sf:form>        <h4>            <small>                <span class="glyphicon glyphicon-book"><!-- book --></span> Subject: <em><a href="${subj}/${announcement.subject.code}">${announcement.subject.code}</a></em> |                <span class="glyphicon glyphicon-time"><!-- time --></span> Posted on: <em>${announcement.timeOfPost}</em> |                <span class="glyphicon glyphicon-time"><!-- time --></span> Last modified: <em>${announcement.lastModified}</em>            </small>        </h4>        <hr />        ${announcement.message}    </div> <!-- /container --></jsp:root>