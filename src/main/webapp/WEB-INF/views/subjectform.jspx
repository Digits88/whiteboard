<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/subject/add" var="form_url"/>    <div class="container">        <div class="page-header">            <h3>Subject Form</h3>        </div>        <sf:form method="post" action="${form_url}" modelAttribute="subject">            <div class="form-group row">                <label for="inputCode" class="col-sm-2 col-form-label">Code</label>                <div class="col-sm-5">                    <sf:input id="inputCode" path="code" class="form-control"/>                    <sf:errors path="code" cssclass="error"/>                </div>            </div>            <div class="form-group row">                <label for="inputName" class="col-sm-2 col-form-label">Name</label>                <div class="col-sm-5">                    <sf:input id="inputName" path="name" class="form-control"/>                    <sf:errors path="name" cssclass="error"/>                </div>            </div>            <div class="form-group row">                <label for="inputDepartment" class="col-sm-2 col-form-label">Department</label>                <div class="col-sm-5">                    <sf:input id="inputDepartment" path="department" class="form-control"/>                    <sf:errors path="department" cssclass="error"/>                </div>            </div>            <div class="btn-toolbar">                <button class="btn btn-primary">Submit</button>                <a href="javascript: history.go(-1)" class="btn btn-default">Cancel</a>            </div>        </sf:form>    </div> <!-- /container --></jsp:root>