<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"          xmlns:jsp="http://java.sun.com/JSP/Page"          xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:s="http://www.springframework.org/tags"          xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/spring_security_login_check" var="form_url"/>    <div class="container">        <c:if test="${!empty param.logout and param.logout}">            <div class="alert alert-success" role="alert">                You have successfully logout.            </div>        </c:if>        <c:if test="${!empty param.error and param.error}">            <div class="alert alert-danger" role="alert">                Username or password is incorrect. Try again!            </div>        </c:if>        <div class="page-header">            <h3>LMS Login</h3>        </div>        <sf:form method="post" action="${form_url}" modelAttribute="loginFormDto">            <div class="form-group row">                <label for="inputUsername" class="col-sm-2 col-form-label">Username</label>                <div class="col-sm-5">                    <sf:input id="inputUsername" path="username" class="form-control"/>                    <sf:errors path="username" cssclass="error"/>                </div>            </div>            <div class="form-group row">                <label for="inputPwd" class="col-sm-2 col-form-label">Password</label>                <div class="col-sm-5">                    <sf:password id="inputPwd" path="password" class="form-control"/>                    <sf:errors path="password" cssclass="error"/>                </div>            </div>            <div class="btn-toolbar">                <button class="btn btn-primary">Login</button>            </div>            <input type="hidden" name="${_csrf.parameterName}" value="${_csrf.token}"/>        </sf:form>        <div class="page-header">            <h3>For Demo</h3>        </div>        <table class="table table-striped">            <thead>            <tr>                <th>Username</th>                <th>Password</th>                <th>Role</th>                <th>Class</th>            </tr>            </thead>            <tbody>                <tr>                    <td>admin</td>                    <td>123456</td>                    <td>ROLE_ADMIN</td>                    <td>**Special User** (dev purpose only; not part of LMS)</td>                </tr>                <tr>                    <td>victor</td>                    <td>123456</td>                    <td>ROLE_STUDENT</td>                    <td>Student</td>                </tr>                <tr>                    <td>regina</td>                    <td>123456</td>                    <td>ROLE_STUDENT</td>                    <td>Student</td>                </tr>                <tr>                    <td>alex</td>                    <td>123456</td>                    <td>ROLE_LECTURER</td>                    <td>Staff</td>                </tr>                <tr>                    <td>eman</td>                    <td>123456</td>                    <td>ROLE_LECTURER</td>                    <td>Staff</td>                </tr>                <tr>                    <td>bob</td>                    <td>123456</td>                    <td>ROLE_ADMIN</td>                    <td>Staff</td>                </tr>                <tr>                    <td>nick</td>                    <td>123456</td>                    <td>ROLE_ADMIN</td>                    <td>Staff</td>                </tr>            </tbody>        </table>    </div> <!-- /container --></jsp:root>