<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <div class="container">        <h3>${tm.title}</h3>        <sf:form method="post" action="${tm.id}/delete">            <div class="form-group btn-toolbar">                <a class="btn btn-xs btn-success" role="button" href="${tm.id}/update">Update</a>                <button class="btn btn-xs btn-danger">Delete</button>            </div>        </sf:form>        <h4>            <small>Posted on: <em>${tm.timeOfPost}</em> |                Last modified: <em>${tm.lastModified}</em> |                Type: <em>${tm.type.shortName}</em>            </small>        </h4>        <hr />        ${tm.description}    </div> <!-- /container --></jsp:root>