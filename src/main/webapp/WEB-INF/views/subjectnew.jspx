<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/subject/add" var="form_url"/>    <div class="container">        <h3>Add New Subject</h3>        <sf:form method="POST" action="${form_url}" modelAttribute="subject">            <table>                <tr>                    <td><sf:label path="name">Name</sf:label></td>                    <td><sf:input path="name"/><sf:errors path="name" cssclass="error"/></td>                </tr>                <tr>                    <td><sf:label path="code">Code</sf:label></td>                    <td><sf:input path="code"/><sf:errors path="code" cssclass="error"/></td>                </tr>                <tr>                    <td><sf:label path="department">Department</sf:label></td>                    <td><sf:input path="department"/></td>                </tr>                <tr>                    <td><input type="submit" value="Submit"/></td>                </tr>            </table>        </sf:form>    </div> <!-- /container --></jsp:root>