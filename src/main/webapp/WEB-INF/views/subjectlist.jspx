<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <div class="container">        <h3>Subject List</h3>        <c:choose>            <c:when test="${empty subjects}">                <ul>                    <li>No subject availabe in database. <a href="new">Add subject</a>.</li>                </ul>            </c:when>            <c:otherwise>                <ul>                    <c:forEach var="s" items="${subjects}">                        <li><a href="${s.code}">${s.code} : ${s.name}</a></li>                    </c:forEach>                    <li><a href="new">Add subject</a></li>                </ul>            </c:otherwise>        </c:choose>    </div> <!-- /container --></jsp:root>