<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/announcement/add" var="form_url"/>    <s:url value="/subject/new" var="subj_new"/>    <div class="container">        <div class="page-header">            <h3>Announcement Form</h3>        </div>        <c:choose>            <c:when test="${empty subjectList}">                No subject found. <a href="${subj_new}">Add some subject first</a>.            </c:when>            <c:otherwise>                <sf:form method="POST" action="${form_url}" modelAttribute="announcement">                    <sf:hidden path="id"/>                    <sf:hidden path="timeOfPost"/>                    <sf:hidden path="lastModified"/>                    <div class="form-group row">                        <label for="inputTitle" class="col-sm-2 col-form-label">Title</label>                        <div class="col-sm-5">                            <sf:input id="inputTitle" path="title" size="60" class="form-control"/>                            <sf:errors path="title" cssclass="error"/>                        </div>                    </div>                    <div class="form-group row">                        <label for="text" class="col-sm-2 col-form-label">Message</label>                        <div class="col-sm-5">                            <sf:textarea id="text" class="form-control" path="message" rows="10" cols="60"/>                            <h6 class="pull-right" id="count_message"/>                            <sf:errors path="message" cssclass="error"/>                        </div>                    </div>                    <div class="form-group row">                        <label for="inputSubject" class="col-sm-2 col-form-label">Subject</label>                        <div class="col-sm-5">                            <sf:select id="inputSubject" class="form-control" path="subject"                                       items="${subjectList}" itemValue="code" itemLabel="code"/>                            <sf:errors path="subject" cssclass="error"/>                        </div>                    </div>                    <div class="btn-toolbar">                        <button class="btn btn-primary">Submit</button>                        <a href="javascript: history.go(-1)" class="btn btn-default">Cancel</a>                    </div>                </sf:form>            </c:otherwise>        </c:choose>    </div> <!-- /container --></jsp:root>