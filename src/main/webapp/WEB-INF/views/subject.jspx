<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/announcement/new?subjectCode=${subject.code}" var="add_anno"/>    <s:url value="/announcement" var="view_anno"/>    <div class="container">        <h3>${subject.code} ${subject.name}</h3>        <h4>Announcements</h4>        <c:choose>            <c:when test="${empty announcements}">                <ul>                    <li>No announcements. <a href="${add_anno}">Add announcement</a>.</li>                </ul>            </c:when>            <c:otherwise>                <ul>                    <c:forEach var="a" items="${announcements}">                        <li><a href="${view_anno}/${a.id}">${a.title}</a></li>                    </c:forEach>                    <li><a href="${add_anno}">Add announcement</a></li>                </ul>            </c:otherwise>        </c:choose>    </div> <!-- /container --></jsp:root>