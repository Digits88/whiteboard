<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/announcement/new?subjectCode=${subject.code}" var="add_anno"/>    <s:url value="/announcement" var="view_anno"/>    <s:url value="/tm/new?subjectCode=${subject.code}" var="add_tm"/>    <s:url value="/tm" var="view_tm"/>    <div class="container">        <h3>${subject.code} ${subject.name}</h3>        <sf:form method="post" action="${subject.code}/delete">            <div class="form-group btn-toolbar">                <div class="btn-group">                    <button class="btn btn-xs btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Create                        <span class="caret"></span></button>                    <ul class="dropdown-menu">                        <li><a href="${add_anno}">Announcement</a></li>                        <li><a href="${add_tm}">Teaching Material</a></li>                        <li class="disabled"><a href="#">Assessment Submission</a></li>                    </ul>                </div>                <a class="btn btn-xs btn-success" role="button" href="${subject.code}/update">Update</a>                <button class="btn btn-xs btn-danger">Delete</button>            </div>        </sf:form>        <c:if test="${!empty deletedAnnouncement}">            <div class="alert alert-success" role="alert">                Announcement <strong>${deletedAnnouncement}</strong> has successfully deleted.            </div>        </c:if>        <c:if test="${!empty deletedTeachingMaterial}">            <div class="alert alert-success" role="alert">                Teaching Material <strong>${deletedTeachingMaterial}</strong> has successfully deleted.            </div>        </c:if>        <!-- announcement section -->        <div class="page-header">            <h4>Announcements</h4>        </div>        <c:choose>            <c:when test="${empty announcements}">                <ul>                    <li>No announcements. <a href="${add_anno}">Create announcement</a>.</li>                </ul>            </c:when>            <c:otherwise>                <ul>                    <c:forEach var="a" items="${announcements}">                        <li><a href="${view_anno}/${a.id}">${a.title}</a></li>                    </c:forEach>                </ul>            </c:otherwise>        </c:choose>        <div class="page-header">            <h4>Teaching Materials</h4>        </div>        <!-- lecture section -->        <h5>Lecture Notes</h5>        <c:choose>            <c:when test="${empty lectureNotes}">                <ul>                    <li>No lecture notes. <a href="${add_tm}">Create lecture note</a>.</li>                </ul>            </c:when>            <c:otherwise>                <ul>                    <c:forEach var="ln" items="${lectureNotes}">                        <li><a href="${view_tm}/${ln.id}">${ln.title}</a></li>                    </c:forEach>                </ul>            </c:otherwise>        </c:choose>        <!-- workshop section -->        <h5>Workshop Notes</h5>        <c:choose>            <c:when test="${empty workshopNotes}">                <ul>                    <li>No workshop notes. <a href="${add_tm}">Create workshop note</a>.</li>                </ul>            </c:when>            <c:otherwise>                <ul>                    <c:forEach var="ln" items="${workshopNotes}">                        <li><a href="${view_tm}/${ln.id}">${ln.title}</a></li>                    </c:forEach>                </ul>            </c:otherwise>        </c:choose>        <!-- assessment section -->        <h5>Assessment Notes</h5>        <c:choose>            <c:when test="${empty assessmentNotes}">                <ul>                    <li>No assessment notes. <a href="${add_tm}">Create assessment note</a>.</li>                </ul>            </c:when>            <c:otherwise>                <ul>                    <c:forEach var="ln" items="${assessmentNotes}">                        <li><a href="${view_tm}/${ln.id}">${ln.title}</a></li>                    </c:forEach>                </ul>            </c:otherwise>        </c:choose>        <div class="page-header">            <h4>Assessment Submissions</h4>        </div>        <ul>            <li>Under construction</li>        </ul>    </div> <!-- /container --></jsp:root>