<jsp:root version="2.0" xmlns="http://www.w3.org/1999/xhtml"        xmlns:jsp="http://java.sun.com/JSP/Page"        xmlns:c="http://java.sun.com/jsp/jstl/core"        xmlns:s="http://www.springframework.org/tags"        xmlns:sf="http://www.springframework.org/tags/form">    <s:url value="/admin/${username}/comments/add" var="addcmt"/>    <div class="container-fluid">        <div class="row">            <div class="col-sm-3 col-md-2 sidebar">                <ul class="nav nav-sidebar">                    <li class="active"><a href="#">Overview <span class="sr-only">(current)</span></a></li>                    <li><a href="#">Reports</a></li>                    <li><a href="#">Analytics</a></li>                    <li><a href="#">Export</a></li>                </ul>                <ul class="nav nav-sidebar">                    <li><a href="">Nav item</a></li>                    <li><a href="">Nav item again</a></li>                    <li><a href="">One more nav</a></li>                    <li><a href="">Another nav item</a></li>                    <li><a href="">More navigation</a></li>                </ul>                <ul class="nav nav-sidebar">                    <li><a href="">Nav item again</a></li>                    <li><a href="">One more nav</a></li>                    <li><a href="">Another nav item</a></li>                </ul>            </div>            <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">                <!--<h1 class="page-header">Dashboard</h1>-->                <h2 class="sub-header">Add Comment to "${username}"</h2>                <sf:form method="POST" action="${addcmt}" modelAttribute="comment">                    <!--                        this hidden primary key is required, in order to use saveOrUpdate()                        and; instead of generating new Comment if editing the existing instance.                    -->                    <sf:hidden path="id"/>                    <sf:hidden path="timeOfPost"/>                    <!--<sf:hidden path="lecturer"/>-->                    <div class="form-group row">                        <label for="text" class="col-sm-2 col-form-label">Message</label>                        <div class="col-sm-5">                            <sf:textarea id="text" class="form-control" path="message" rows="10" cols="60"/>                            <h6 class="pull-right" id="count_message"/>                            <sf:errors path="message" cssclass="error"/>                        </div>                    </div>                    <div class="btn-toolbar">                        <button class="btn btn-primary">Submit</button>                        <!--<a href="javascript: history.go(-1)" class="btn btn-default">Cancel</a>-->                    </div>                </sf:form>            </div>        </div>    </div></jsp:root>